{
  "name": "feverone-localhost",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "prestart": "npm run pull",
    "start": "npm run update",
    "update": "sudo docker stack deploy -c docker-compose.yml web",
    "list": "sudo docker stack services web",
    "stop": "sudo docker stack down web",
    "pull": "node scripts/pull.js",
    "nginx_reload": "sudo docker exec $(sudo docker container ls --filter 'label=name=web' --format \"{{.ID}}\") nginx -s reload"
  },
  "keywords": [],
  "author": "shynome",
  "dependencies": {
    "js-yaml": "^3.11.0",
    "nodemailer": "^4.6.3",
    "shelljs": "^0.8.1"
  },
  "devDependencies": {
    "@types/node": "^9.4.7",
    "@types/shelljs": "^0.7.8"
  }
}
